{
    "manifest_version": 3,
    "name": "Vite Extension Tutorial",
    "version": "1.0.0",
    "action": {
        "default_popup": "index.html",
        "default_icon": {
            "16": "resources/icon16.png",
            "32": "resources/icon32.png",
            "128": "resources/icon128.png"
        }
    },
    "permissions": [
        "scripting"
    ],
    "host_permissions": [
        "http://*/*",
        "https://*/*"
    ]
}