{
    "manifest_version": 3,
    "name": "Get Back to Work",
    "version": "1.0.0",
    "action": {
        "default_popup": "index.html"
    },
    "background": {
        "service_worker": "service-worker.js",
        "type": "module"
    },
    "permissions": [
        "webNavigation",
        "tabs",
        "storage",
        "scripting"
    ],
    "host_permissions": [
        "http://*/*",
        "https://*/*",
        "http://www.facebook.com/*",
        "https://www.facebook.com/*"
    ]
}